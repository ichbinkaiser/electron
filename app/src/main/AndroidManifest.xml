<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="ichbinkaiser.electron">

    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/pong"
        android:label="@string/app_name">
        <activity
            android:name="ichbinkaiser.electron.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="ichbinkaiser.electron.MainActivity">
        </activity>
        <activity
            android:name="ichbinkaiser.electron.ScoreActivity">
        </activity>
        <activity
            android:name="ichbinkaiser.electron.GameActivity">
        </activity>

    </application>

</manifest>
